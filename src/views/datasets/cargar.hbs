<div class="container p-4">

    <div class="row">
        <div class="col-md-4 mx-auto">
            <div class="card">

                <div class="card-body">

                    <h3>Subir Dataset</h3>

                    <form action="/subir" method="POST" enctype="multipart/form-data">
                        <div class="form-group">
                            <input type="text" class="form-control" name="nombre" placeholder="Nombre archivo" autofocus>
                        </div>
                        <br>
                        <div class="form-group">
                            <input type="text" class="form-control" name="tipo" placeholder="Tipo archivo" autofocus>
                        </div>
                        <br>
                        <div class="form-group">
                            <input type="file" name="avatar" id="">
                        </div>
                        <br>
                        <div class="form-group">
                            <textarea name="descripcion" id="" rows="2" class="form-control"
                                placeholder="Descripcion"></textarea>
                        </div>

                        <br>
                        <div class="form-group">
                            <button class="btn btn-success col-12">
                                Atras
                            </button>
                        </div>
                        <br>
                        <div class="form-group">
                            <button class="btn btn-success col-12">
                                Save
                            </button>
                        </div>
                    </form>

                </div>



            </div>
        </div>
    </div>
</div>




